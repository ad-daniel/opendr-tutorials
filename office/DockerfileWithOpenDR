FROM cyberbotics/webots.cloud:R2022b-ubuntu20.04
RUN apt update && apt --option="APT::Acquire::Retries=3" install -y git python3-dev python3-pip libfreetype6-dev build-essential cmake python3-dev wget libopenblas-dev libsndfile1 libboost-dev && python3 -m pip install wheel protobuf==3.20.0 onnx==1.8.0 numpy==1.23.5 opendr-toolkit-engine && python3 -m pip install git+https://github.com/cocodataset/panopticapi.git#egg=panopticapi && python3 -m pip install opendr-toolkit-object-detection-2d